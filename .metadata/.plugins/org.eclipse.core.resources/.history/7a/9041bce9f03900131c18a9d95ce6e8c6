package intermediate;

public class token {
String value ;
TokenType type ;
	public token(String s,TokenType tt){
		value= s;
		type=tt;
	}
	public String getValue() {
		return value;
	}
	public void setValue(String value) {
		this.value = value;
	}
	public TokenType getType() {
		return type;
	}
	public void setType(TokenType type) {
		this.type = type;
	}
}
