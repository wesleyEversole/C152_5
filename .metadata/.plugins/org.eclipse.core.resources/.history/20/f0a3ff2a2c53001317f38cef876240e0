package intermediate;

public class TopLvlItem {
	Tree MT;
	SymbolTable mainForm;

	public TopLvlItem(Tree t, SymbolTable st) {
		MT = t;
		mainForm = st;
	}

	public Tree getMT() {
		return MT;
	}

	public void setMainForm(Tree mT) {
		MT = mT;
	}

	public SymbolTable getMST() {
		return mainForm;
	}

	public void setMST(SymbolTable mST) {
		mainForm = mST;
	}
}
